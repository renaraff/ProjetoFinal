@model ProjetoFinal.Models.EntradaProduto

@{
    ViewData["Title"] = "Detalhes";
}

<h1>Detalhes da entrada do produto</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Produto)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Produto.ProdutoNome)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EntradaProdutoData)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EntradaProdutoData)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EntradaProdutoQuantidade)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EntradaProdutoQuantidade)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Editar</a> |
    <a asp-action="Index">Voltar</a>
</div>
